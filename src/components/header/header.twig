      <ul class="languageLinks">
        <li>
          <a href="/">
            <img src="images/ico/cz.svg" alt="language" />
          </a>
        </li>
        <li>
          <a href="/en/">
            <img src="images/ico/gb.svg" alt="language" />
          </a>
        </li>
        <li>
          <a href="/de/">
            <img src="images/ico/de.svg" alt="language" />
          </a>
        </li>
      </ul>
